{
    "name": "whitedev/filament-permissions",
    "description": "Filament + Spatie dynamic permissions package for CRUD",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WhiteDev\\FilamentPermissions\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0 || ^12.0",
        "filament/filament": "^3.0",
        "spatie/laravel-permission": "^6.0",
        "white-dev/filament-permissions": "dev-main"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/aktilokchandani/filament-spatie-permissions"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "WhiteDev\\FilamentPermissions\\FilamentPermissionsServiceProvider",
                "WhiteDev\\FilamentPermissions\\PanelProvider"
            ]
        }
    }
}
